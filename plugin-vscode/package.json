{
	"name": "flutter-local-storage-inspector",
	"displayName": "Flutter local storage inspector",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:flutter-local-storage-inspector.inspect-storage",
		"onView:flutter-local-storage-inspector-view"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "flutter-local-storage-inspector.inspect-storage",
				"title": "Flutter inspect local storage"
			}
		],
		"viewsContainers": {
			"panel": [
				{
					"id": "flutter-local-storage-inspector",
					"title": "Local storage",
					"icon": "media/app.svg"
				}
			]
		},
		"views": {
			"flutter-local-storage-inspector": [
				{
					"type": "webview",
					"id": "flutter-local-storage-inspector-view",
					"name": "test"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}